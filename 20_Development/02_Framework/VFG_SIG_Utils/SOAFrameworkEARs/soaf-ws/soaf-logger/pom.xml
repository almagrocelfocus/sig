<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>soaf-logger</artifactId>
	
	<parent>
		<artifactId>soaf-ws</artifactId>
		<groupId>com.vodafone.soaf.ws</groupId>
		<version>0.1.0</version>
	</parent>
	
	<packaging>pom</packaging>
	<name>soaf-logger</name>
	
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		
		<maven.build.timestamp.format>yyyyMMddHHmm</maven.build.timestamp.format>
		
		<build.debug>true</build.debug>
		<build.quiet>true</build.quiet>
		<build.skipTests>true</build.skipTests>
	</properties>
	
	<modules>
		<module>soaf-logger-ear</module>
		<module>soaf-logger-db</module>
		<module>soaf-logger-lib</module>
	</modules>
	
	<dependencies>
		<dependency>
			<groupId>com.eai.common</groupId>
			<artifactId>eai-common-lib</artifactId>
			<version>${dependency.common.eai.version}</version>
			<type>ejb</type>
		</dependency>
	</dependencies>
	
</project>
